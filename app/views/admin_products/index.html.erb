<h3>商品一覧</h3>
<table>
    <thead>
        <tr>
            <th>商品ID</th>
            <th>商品名</th>
            <th>税抜価格</th>
            <th>ジャンル</th>
            <th>ステータス</th>
        </tr>
    </thead>
    <tbody>
        
            <% @products.each do |product| %>
                <tr>
                    <td><%= product.id %></td>
                    <td><%= product.name %></td>
                    <td><%= product.notax_price %></td>
                    <td></td>
                    <td></td>
                </tr>
            <% end %>
    </tbody>
</table>

<%= link_to "ログアウト", user_registration_path, method: :delete %>